{
  "manifest_version": 2,

  "name": "Chrome Background - alpha",
  "description": "This extension automates the background change process of chrome's homepage",
  "version": "0.1.1",

  "browser_action": {
    "default_popup": "popup.html"
  },
    
    "content_scripts":[
        {
            "matches": ["https://www.google.co.in/"],
            "js":["background_trigger.js"]
        }
    ],
  "permissions": [
    "activeTab",
    "storage"
  ]
}